<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
  <body>
    <%- include('_navBar')%>
    <h1>UserProfile</h1>



    <table class="table table-striped container my-5">
        <tr>
            <th>fullName</th>
            <th>city</th>
            <th>job</th>
            <th>height</th>
            <th>weight</th>
            <th>age</th>
        </tr>
         <tr>
            <td><%= data.fullName %></td>
            <td><%= data.city %></td>
            <td><%= data.job %></td>
            <td><%= data.height %></td>
            <td><%= data.weight %></td>
            <td><%= data.age %></td>
         </tr>

      </table>
      <% dataPost.Posts.forEach(el => { %>
        <div class="container">
            <div class="row">
              <div class="col-md-6 offset-md-3">
                <div class="card mt-3">
                  <img src="<%= el.imgUrl %>" class="card-img-top" alt="Post Image">
                  <div class="card-body">
                    <h5 class="card-title"><%= el.title %></h5>
                    <p class="card-text"><%= el.caption %></p>
                    <a href="/like/<%= el.id %>" class="btn btn-primary">Like</a>
                    <a href="#" class="btn btn-secondary"><%= el.like %></a>
                    <a href="" class="btn btn-info">Visit</a>
                  </div>
                </div>
              </div>
            </div>
          </div>

    <% }) %>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>